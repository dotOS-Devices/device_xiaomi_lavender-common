genfscon sysfs /devices/soc/soc:fingerprint_fpc/irq_enable      u:object_r:fingerprint_sysfs:s0
genfscon sysfs /devices/soc/soc:fingerprint_fpc/wakeup_enable   u:object_r:fingerprint_sysfs:s0
